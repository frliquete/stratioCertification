CREATE TABLE `risk_scoring` (`ID` STRING, `children` STRING, `current_debt` STRING, `year` STRING, `age` STRING, `sex` STRING, `job` STRING, `housing` STRING, `saving_accounts` STRING, `checking_account` STRING, `credit_amount` STRING, `duration` STRING, `purpose` STRING, `risk` STRING) USING org.apache.spark.sql.parquet OPTIONS ( path 'hdfs://tenantproducer-hdfs-retail/use-cases/risk-scoring/parquet/clients_historic_raw' ) 
